    "lint:watch": "npm run lint -- --watch",
    "server": "npm run clean && cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm-run-all --parallel server lint:watch",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
